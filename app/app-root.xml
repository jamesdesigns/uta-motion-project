<TabView tabTextFontSize="26" selectedTabTextColor="red" tabBackgroundColor="white" androidTabsPosition="bottom" style="font-family:FontAwesome;" class="p-t-5 fa iconFont">
 
    <TabViewItem class="p-t-5" title="&#xff239;">
            <Frame defaultPage="home/home-items-page"></Frame>
    </TabViewItem>

    <TabViewItem class="p-t-5" title="&#xff041;">
            <Frame defaultPage="map/map-page"></Frame>  
    </TabViewItem>

    <TabViewItem class="p-t-5" title="&#xff004;">
        <Frame defaultPage="favorites/favorites-page"></Frame>
    </TabViewItem>

    <TabViewItem class="-t-5" title="&#xff071;">
            <Frame defaultPage="alerts/alerts-page"></Frame>
    </TabViewItem>
</TabView>
